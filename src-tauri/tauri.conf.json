{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:5173",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "vmboost",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "all": true,
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "copyFile": true,
        "createDir": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "exists": true,
        "scope": [
          "**"
        ]
      },
      "dialog": {
        "all": true, 
        "ask": true, 
        "confirm": true, 
        "message": true, 
        "open": true, 
        "save": true 
      },
      "http": {
        "all": true,
        "request": true,
        "scope": ["https://**/*"] 
      },
      "all": false,
      "shell": {
        "all": true,
        "open": true,
        "sidecar": true,
        "execute": true,
        "scope": [{
          "name": "vagrant",
          "binary": "vagrant",
          "cmd": "vagrant",
          "args": ["up"]
        }]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.vmboost.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": ""
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "vmboost",
        "width": 800,
        "height": 600
      }
    ]
  }
}
